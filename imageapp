import 'package:flutter/material.dart';
void main() => runApp(ImageApp());

class ImageApp extends StatelessWidget {
  @override
  Widget build(BuildContext context) {
    return MaterialApp(
      home: Scaffold(
        appBar: AppBar(title: Text("Image Example")),
        body: Center(
          child: Column(
            mainAxisAlignment: MainAxisAlignment.center,
            children: [
              Icon(Icons.favorite, color: Colors.red, size: 60),
              SizedBox(height: 20),
              Image.network('https://www.google.com/url?sa=i&url=https%3A%2F%2Fwww.freepik.com%2Ffree-photos-vectors%2Fimaage&psig=AOvVaw2JJra-o5hEcg0_oL_t22GF&ust=1763998292953000&source=images&cd=vfe&opi=89978449&ved=0CBUQjRxqFwoTCNCNhO_LiJEDFQAAAAAdAAAAABAE', width: 100),
            ],
          ),
        ),
      ),
    );
  }
}
